    <script>	
		//모달창 체크하기
		function modalCheck(form,no){
			var num=parseInt(no);
			var number=parseInt(no)+3;
			var lst=$("input[name='"+form+"']");
			var lstStr="";
			for(var i=0;i<lst.size();i++){
				if(lst.eq(i).prop("checked")){
					lstStr+=lst.eq(i).val()+",";
				}
			}
			
			lstStr=lstStr.substring(0,lstStr.length-1);
			if(5<=num){
				$("#mb-ideal-type").val(lstStr);
			}else{
				$("#mb-ext"+number).val(lstStr);
			}
			if(lstStr.trim()==""){
				lstStr="선택해주세요";
			}
			$("#mb-ext"+number+"-str").html(lstStr);
			$("#myModal"+no).modal('hide');
		}
		//오디오 보여주기
		function audioShow(my_voice){

			var varUA = navigator.userAgent.toLowerCase();
			if (varUA.indexOf("iphone")>-1||varUA.indexOf("ipad")>-1||varUA.indexOf("ipod")>-1) { 
				my_voice = "<?=G5_URL?>/data/tmpfile/" + my_voice;
			}
			var strHtml='<audio controls  preload="" id="my-voice" style="width:100%;">';
					strHtml+='<source src="'+my_voice+'"  type="audio/mp3"/>';
					strHtml+='</audio>';
			//$("#my-voice").html(strHtml);
			$("#my-audio").html(strHtml);
			
			var my_voiceArray=my_voice.split("/");
			var last=my_voiceArray.length-1;
			$("#mb-voice").val(my_voiceArray[last]);
		}
		//사진보여주기
		function photoShow(url,id){
			$("#mb-"+id).attr("src",url);
			var my_photoArray=url.split("/");
			var last=my_photoArray.length-1;
			if(id=="photo1"){
				$(".noimg").eq(0).css("display","none");
				$("#mb-photos1").val(my_photoArray[last]);
			}
			if(id=="photo2"){
				$(".noimg").eq(1).css("display","none");
				$("#mb-photos2").val(my_photoArray[last]);
			}
		}
		//음성변경
		function soundRecord(){
			var varUA = navigator.userAgent.toLowerCase();
			if (varUA.match('android') != null) { 
				//안드로이드 일때 처리
				window.android.soundRecord();
			} else if (varUA.indexOf("iphone")>-1||varUA.indexOf("ipad")>-1||varUA.indexOf("ipod")>-1) { 
				webkit.messageHandlers.getVoice.postMessage("");
			}
		}
		//사진변경
		function file_upload(no){
			var varUA = navigator.userAgent.toLowerCase();
			if (varUA.match('android') != null) { 
				window.android.getPhoto(no);
			}else if(varUA.indexOf("iphone")>-1||varUA.indexOf("ipad")>-1||varUA.indexOf("ipod")>-1) { 
				$("#mb-photoFile"+no).click();
			}else{
				$("#mb-photoFile"+no).click();
			}
		}
		function readURL(input,no) {
			if (input.files && input.files[0]) {
				var reader = new FileReader();
				reader.onload = function(e) {
					$('#mb-photo'+no).attr('src', e.target.result);
					$(".noimg").eq(no-1).css("display","none");
				}
				reader.readAsDataURL(input.files[0]);
			}
		}

